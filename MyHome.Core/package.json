{
  "name": "MyHomeCore",
  "version": "1.0.0",
  "angular-cli": {},
  "dependencies": {
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-uglify": "^1.5.4",
    "gulp-concat": "^2.6.0",
    "gulp-watch": "^4.3.8",
    "gulp-util": "^3.0.7",
    "gulp-batch": "^1.0.5",
    "gulp-jsvalidate": "^2.1.0",
    "gulp-eslint": "^3.0.1",
    "gulp-exec": "^2.1.3"
  },
  "scripts": {
    "backend": "dotnet watch run",
    "bb": "dotnet watch run",
    "frontend": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base app/",
    "ff": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base app/",
    "start": "concurrently \"npm run frontend -- --open\" \"npm run backend\" ",
    "ef-m-add": "dotnet ef migrations add",
    "build-frontend": "webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build-backend": "dotnet build -c Release",
    "publish": "dotnet publish -c Release",
    "release": "npm run build-frontend && npm run build-backend && npm run publish"
  }
}
